<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Practice.Groups.AttendanceWindow">
    <Grid>
        <DataGrid Name="AttendanceDataGrid" x:CompileBindings="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="ID" Binding="{Binding AttendanceID}" />
                <DataGridTextColumn Header="Начало" Binding="{Binding Start}" />
                <DataGridTextColumn Header="Конец" Binding="{Binding End}" />
                <DataGridTextColumn Header="Клиент" Binding="{Binding Client}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
